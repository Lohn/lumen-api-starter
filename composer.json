{
  "name": "mstaack/lumen-api-starter",
  "description": "Lumen Starter for APIs",
  "keywords": [
    "framework",
    "laravel",
    "lumen",
    "json",
    "rest",
    "api"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "laravel/lumen-framework": "6.3.*",
    "vlucas/phpdotenv": "^3.6",
    "flipbox/lumen-generator": "6.*",
    "paragonie/paseto": "^1.0",
    "itsgoingd/clockwork": "^4.0",
    "pearl/lumen-request-validate": "^1.4",
    "illuminate/mail": "^6.0",
    "spatie/laravel-permission": "^3.11",
    "beyondcode/laravel-dump-server": "^1.3",
    "mpociot/laravel-apidoc-generator": "^4.4"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.9",
    "phpunit/phpunit": "~9.0",
    "mockery/mockery": "~1.3",
    "barryvdh/laravel-ide-helper": "^2.6",
    "nunomaduro/collision": "^3.0",
    "codedungeon/phpunit-result-printer": "^0.27",
    "laravel/homestead": "^11.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ],
    "post-root-package-install": [
      "php vendor/bin/homestead make",
      "composer keys",
      "composer meta"
    ],
    "meta": [
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan ide-helper:model",
      "php artisan optimize"
    ],
    "keys": [
      "php artisan key:generate",
      "php artisan auth:generate-paseto-key"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
